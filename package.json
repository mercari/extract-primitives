{
  "name": "extract-primitives",
  "version": "1.0.0",
  "description": "Project description.",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./lib && tsc -p tsconfig.lib.json && cp package.json LICENSE README.md lib",
    "test": "jest",
    "lint": "eslint ./src/**/*.ts --fix",
    "type-check": "tsc --noEmit -p tsconfig.lib.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mercari/extract-primitives.git"
  },
  "keywords": [
    "typescript"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mercari/extract-primitives/issues"
  },
  "homepage": "https://github.com/mercari/extract-primitives#readme",
  "peerDependencies": {
    "typescript": ">=3.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.1",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "jest-snapshot-serializer-raw": "^1.1.0",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.1.1",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run type-check && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.ts": [
      "npm run lint"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
